{
   "background": {
      "scripts": [ "database.js"]
   },
   "browser_action": {
      "browser_style": true,
      "default_popup": "popup/popup.html",
      "default_title": "FMK User Blocker"
   },
   "content_scripts": [ {
      "js": [ "jquery.js", "block.js"],
      "matches": [ "http://*.fmkorea.com/*" ],
      "run_at": "document_end"
   } ],
   "icons": {
      "128": "icon128.png"
   },
   "key": "",
